{
  "entries": ["./src/as/assembly/index.ts"],
  "options": {
    "bindings": "esm",
    "enable": ["reference-types","threads"],
    "runtime": "minimal",
    "exportRuntime": true
  },
  "targets": {
    "release": {
      "optimize": true,
      "outFile": "./build/vite-react-promise-worker-assemblyscript-boilerplate/assets/index.wasm",
      "textFile": "./build/vite-react-promise-worker-assemblyscript-boilerplate/assets/index.wat"
    },
    "debug": {
      "debug": true,
      "outFile": "./build/vite-react-promise-worker-assemblyscript-boilerplate/assets.debug/index.wasm",
      "textFile": "./build/vite-react-promise-worker-assemblyscript-boilerplate/assets/index.wat"
    }
  }
}
